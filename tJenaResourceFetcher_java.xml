	jjzhou $ -->
<COMPONENT>
	<HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="DEVELOPMENT"
    COMPATIBILITY="ALL"
    AUTHOR="Stefan Scheffler"
    RELEASE_DATE="20070525A"
    STARTABLE="true" 
  >
    <SIGNATURE/>
    </HEADER>

	<FAMILIES>
		<FAMILY>semanticWeb/Jena/Fuseki</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_OUTPUT="1" MAX_OUTPUT="1" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" />
	</CONNECTORS>

	<PARAMETERS>
	
	<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="10" READONLY="true">
      <TABLE>
        <COLUMN NAME="subject" TYPE="id_String" />
        <COLUMN NAME="predicate" TYPE="id_String" />
        <COLUMN NAME="object" TYPE="id_String" />
        <COLUMN NAME="objectType" TYPE="id_String" />
      </TABLE>
    </PARAMETER>
    
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tFusekiConnection" NUM_ROW="10" DYNAMIC_SETTINGS="true" />
    
    <PARAMETER NAME="GLOBAL_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="10" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="TYPE_TABLE" FIELD="TABLE" REQUIRED="false" NUM_ROW="20" NB_LINES="3" >
      <ITEMS>
      	<ITEM NAME="TYPE_ITEM" />
      </ITEMS>
    </PARAMETER>
	
	<PARAMETER NAME="GRAPH_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="25">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="GRAPH" FIELD="TEXT" NUM_ROW="25" REQUIRED="true" SHOW_IF="GRAPH_CHECK == 'true'" >
        	<DEFAULT></DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ALL_RADIO" FIELD="RADIO" REQUIRED="true" NUM_ROW="30" GROUP="SELECT">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>
		
	<PARAMETER NAME="INCLUDE_RADIO" FIELD="RADIO" REQUIRED="true" NUM_ROW="30" GROUP="SELECT">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="EXCLUDE_RADIO" FIELD="RADIO" REQUIRED="true" NUM_ROW="30" GROUP="SELECT">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="INCLUDE_TABLE" FIELD="TABLE" REQUIRED="false" NUM_ROW="35" NB_LINES="5" SHOW_IF="INCLUDE_RADIO == 'true'" GROUP="SELECT">
      <ITEMS>
      	<ITEM NAME="INC_PREDICATE_ITEM" />
      </ITEMS>
    </PARAMETER>

	<PARAMETER NAME="EXCLUDE_TABLE" FIELD="TABLE" REQUIRED="false" NUM_ROW="35" NB_LINES="5" SHOW_IF="EXCLUDE_RADIO == 'true'" GROUP="SELECT">
      <ITEMS>
      	<ITEM NAME="EXC_PREDICATE_ITEM" />
      </ITEMS>
    </PARAMETER>
    
    

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
	
	
	<PARAMETER NAME="LIMIT_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="10">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="LIMIT_FIELD" FIELD="TEXT" NUM_ROW="10" REQUIRED="true" SHOW_IF="LIMIT_CHECK == 'true'">
        <DEFAULT>10</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="OFFSET_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="20">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="START_OFFSET_FIELD" FIELD="TEXT" NUM_ROW="20" REQUIRED="true" SHOW_IF="OFFSET_CHECK == 'true'">
        <DEFAULT>0</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="OFFSET_FIELD" FIELD="TEXT" NUM_ROW="20" REQUIRED="true" SHOW_IF="OFFSET_CHECK == 'true'">
        <DEFAULT>1000</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="VERBOSE_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="30">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="fuseki" MODULE="fuseki-server.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>